programa
{
	
	funcao inicio()
	{
		inteiro N1
		inteiro N2
		inteiro Resultado

		escreva("++++++Calculadora de soma de dois números++++++")
		escreva("\nDigite o valor do 1° número: ")
		leia(N1)
		escreva("\nDigite o valor do 2° número: ")
		leia(N2)
		Resultado = N1+N2

		escreva("\nTotal:", Resultado)
		
	}
}
