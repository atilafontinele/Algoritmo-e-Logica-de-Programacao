programa
{
	
	funcao inicio()
	{
		inteiro n1
		inteiro n2
		inteiro soma

		escreva("++++++Calcular Soma++++++")
		escreva("\nEntre com o 1° número: ")
		leia(n1)
		escreva("\nEntre com o 2° número: ")
		leia(n2)
		soma = n1+n2

		escreva("\nTotal:", soma)
		
	}
}
